<Window x:Class="WorkingWithCashe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkingWithCashe"
        mc:Ignorable="d"
        Title="Data from cache" Height="127" Width="203">
    <Grid Margin="0,0,10,14">
        <TextBox x:Name="dataFromCache" HorizontalAlignment="Left" Height="26" Margin="10,10,0,0" TextWrapping="Wrap" Text="Data from cache" VerticalAlignment="Top" Width="165"/>
        <Button x:Name="GetDataFromCache" Content="GET" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Click="GetDataFromCache_Click"/>
        <Button x:Name="SetDataFromCache_Copy" Content="SET" HorizontalAlignment="Left" Margin="10,66,0,0" VerticalAlignment="Top" Click="SetDataFromCache_Copy_Click"/>

    </Grid>
</Window>
